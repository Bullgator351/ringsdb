<ol>
  {% for cycle in data %}
    <li>
      <a href="{{ cycle.url }}">{{ cycle.label }}</a>
      <span>{% if cycle.known != cycle.total %}({{cycle.known}}/{{cycle.total}}){% endif %}</span>

      {% if cycle.packs|length > 1 %}
        <ol>
          {% for pack in cycle.packs %}
            <li>
              <a href="{{ pack.url }}">{{ pack.label }}</a>
              <span>{% if pack.known != pack.total %}({{pack.known}}/{{pack.total}}){% endif %}</span>
            </li>

          {% endfor %}
        </ol>
      {% endif %}
    </li>
  {% endfor %}
</ol>
